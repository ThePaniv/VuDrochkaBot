{
  "family": "VudrochkaBot",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::126568927381:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "vudrochka-bot",
      "image": "126568927381.dkr.ecr.eu-central-1.amazonaws.com/vudrochka-bot:latest",
      "cpu": 256,
      "memory": 512,
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/vudrochka-bot",
          "awslogs-region": "eu-central-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "taskRoleArn": "arn:aws:iam::126568927381:role/ecsTaskExecutionRole"
}
